{
  "name": "tubetape",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun run build:yt-dlp && bun run build:ffmpeg && tsc && vite build",
    "build:ffmpeg": "bun run scripts/setup-ffmpeg.ts",
    "build:yt-dlp": "bun run scripts/setup-yt-dlp.ts",
    "update": "bun run update:yt-dlp && bun run update:ffmpeg",
    "update:yt-dlp": "bun run scripts/update-yt-dlp.ts --update",
    "update:ffmpeg": "bun run scripts/update-ffmpeg.ts --update-all",
    "clean": "bun run clean:yt-dlp && bun run clean:ffmpeg",
    "clean:ffmpeg": "bun run scripts/setup-ffmpeg.ts --clean",
    "clean:yt-dlp": "bun run scripts/setup-yt-dlp.ts --clean",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint",
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "ladle:preview": "ladle preview -p 61000",
    "test": "bun run test:web && bun run test:tauri",
    "test:web": "vitest run",
    "test:tauri": "cargo test --manifest-path src-tauri/Cargo.toml",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:visual": "playwright test",
    "test:visual:update": "bun run ladle:build && playwright test -u",
    "test:visual:report": "playwright show-report .ladle/playwright-report"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
