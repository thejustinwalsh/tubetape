{
  "name": "tubetape",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint",
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "ladle:preview": "ladle preview -p 61000",
    "test": "bun run test:web && bun run test:tauri",
    "test:web": "vitest run",
    "test:tauri": "cargo test --manifest-path src-tauri/Cargo.toml",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:visual": "playwright test",
    "test:visual:update": "bun run ladle:build && playwright test -u",
    "test:visual:report": "playwright show-report .ladle/playwright-report"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-opener": "^2",
    "ajv": "^8.17.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rxdb": "^16.21.1",
    "rxjs": "^7.8.2",
    "wavesurfer.js": "7.12.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ladle/react": "^5.1.1",
    "@playwright/test": "^1.57.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.16.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/semver": "^7.7.1",
    "@types/unzipper": "^0.10.9",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "^1.0.0",
    "bun-types": "^1.3.5",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "conventional-commits-parser": "^6.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "semver": "^7.7.3",
    "tailwindcss": "4.1.18",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.51.0",
    "unzipper": "^0.12.3",
    "vite": "^7.0.4",
    "vitest": "^4.0.16"
  },
  "resolutions": {
    "zod": "^4.0.0",
    "zod-validation-error": "^4.0.0"
  },
  "patchedDependencies": {
    "wavesurfer.js@7.12.1": "patches/wavesurfer.js@7.12.1.patch"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ]
    ]
  },
  "binaryDeps": {
    "deno": {
      "type": "release",
      "repo": "denoland/deno"
    },
    "quickjs": {
      "type": "source",
      "repo": "bellard/quickjs",
      "sha": "de4d3927b8edff5fbfee1f69cfeef840844259e9"
    },
    "ffmpeg": {
      "type": "source",
      "repo": "FFmpeg/FFmpeg",
      "sha": "649a4e98f40feceea6c6bd933040b6ba9583980e"
    }
  }
}
